{
  "name": "devignator-drew-11ty",
  "version": "1.0.0",
  "description": "personal site of Andrew Harris",
  "main": "index.js",
  "scripts": {
    "serve": "yarn dev",
    "start": "yarn dev",
    "build": "npm-run-all --parallel build:*",
    "build:11ty": "eleventy",
    "build:css": "parcel build src/css/* --out-dir _site",
    "dev": "npm-run-all --parallel dev:*",
    "dev:11ty": "eleventy --serve",
    "dev:css": "parcel src/css/* --out-dir _site",
    "watch": "eleventy --watch",
    "debug": "DEBUG=* eleventy"
  },
  "keywords": [
    "blog"
  ],
  "extends": [
    "@commitlint/config-conventional"
  ],
  "dependencies": {
    "@11ty/eleventy": "^0.11.0",
    "@commitlint/cli": "^9.1.2",
    "autoprefixer": "^9.8.6",
    "npm-run-all": "^4.1.5",
    "parcel": "^1.12.4",
    "tailwindcss": "^1.8.6"
  },
  "license": "MIT",
  "devDependencies": {
    "husky": "^4.3.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
